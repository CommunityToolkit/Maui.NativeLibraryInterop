            xmlns:local="clr-namespace:MauiFirebaseMessagingSample;assembly=Sample"
            x:Class="MauiFirebaseMessagingSample.AppTabbedPage">

    <NavigationPage Title="Analytics">
        <x:Arguments>
            <local:FIRAnalyticsPage Title="Analytics" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Auth">
        <x:Arguments>
            <local:FIRAuthPage Title="Auth" />
